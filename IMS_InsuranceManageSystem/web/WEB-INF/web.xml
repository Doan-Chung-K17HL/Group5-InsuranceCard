<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>blog_add</servlet-name>
        <servlet-class>Controller.blog.blog_add</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_IP_list</servlet-name>
        <servlet-class>Controller.admin.admin_IP_list</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_IP_add</servlet-name>
        <servlet-class>Controller.admin.admin_IP_add</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_user_list</servlet-name>
        <servlet-class>admin_user_list</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test2</servlet-name>
        <servlet-class>Controller.admin.test2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>blog_add</servlet-name>
        <url-pattern>/blog_add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_IP_list</servlet-name>
        <url-pattern>/admin_IP_list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_IP_add</servlet-name>
        <url-pattern>/admin_IP_add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_user_list</servlet-name>
        <url-pattern>/admin_user_list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test2</servlet-name>
        <url-pattern>/test2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>
            Home.jsp
        </welcome-file>
    </welcome-file-list>
</web-app>
